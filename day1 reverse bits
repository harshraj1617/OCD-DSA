 class Solution {
        public:
          long long reversedBits(long long x) {
              long long r=0;
              for(int i=0;i<32;i++)
              {
                  long long b=x&1;
                  x=x>>1;
                  r=r<<1;
                  r=r|b;
                  
              }
              return r;
          }
      };
      
   
